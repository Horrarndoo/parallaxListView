<?xml version="1.0" encoding="utf-8"?>
<com.zyw.horrarndoo.parallaxlistview.view.parallaxview.GradientLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/gl_content"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.zyw.horrarndoo.parallaxlistview.MainActivity">

    <com.zyw.horrarndoo.parallaxlistview.view.parallaxview.ParallaxListView
        android:id="@+id/lv_parallax"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="none"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">
    </com.zyw.horrarndoo.parallaxlistview.view.parallaxview.ParallaxListView>

    <include layout="@layout/title_bar_layout"/>
</com.zyw.horrarndoo.parallaxlistview.view.parallaxview.GradientLayout>
